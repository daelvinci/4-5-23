@model ViewModel
<main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <section id="breadcrumbs" class="breadcrumbs">
        <div class="container">

            <ol>
                <li><a href="index.html">Home</a></li>
                <li>Team</li>
            </ol>
            <h2>Team</h2>

        </div>
    </section><!-- End Breadcrumbs -->
    <!-- ======= Team Section ======= -->
    <section id="team" class="team">
        <div class="container">

            <div class="row">
                @foreach (var item in Model.TeamMembers)
                {

                    <div class="col-lg-4 col-md-6 d-flex align-items-stretch">
                        <div class="member">
                            <img src="~/assets/img/team/@item.BgImg" alt="">
                            <h4>@item.Title</h4>
                            <span>@item.Position</span>
                            <p>
                            </p>
                            <div class="social">
                                @foreach (var scm in Model.SocialMedias)
                                {
                                    <a href="@scm.Url"><i class="@scm.Icon"></i></a>
                                }
                            </div>
                        </div>
                    </div>
                }


            </div>

        </div>
    </section><!-- End Team Section -->

</main><!-- End #main -->