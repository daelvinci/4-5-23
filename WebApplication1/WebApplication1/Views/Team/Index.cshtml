                                <a href="@Model[i].SocialMedias[i].Url"><i class="@Model[i].SocialMedias[i].Icon"></i></a>
                                <a href="@Model[i].SocialMedias[i].Url"><i class="@Model[i].SocialMedias[i].Icon"></i></a>
                                <a href="@Model[i].SocialMedias[i].Url"><i class="@Model[i].SocialMedias[i].Icon"></i></a>
                            </div>
                        </div>
                    </div>

                }

            </div>

        </div>
    </section><!-- End Team Section -->

